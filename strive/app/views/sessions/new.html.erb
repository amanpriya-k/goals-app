<h1>Log in below</h1>

<form action="<%= session_url %>" method="post">
  
  <%= auth_token %>
  
  <label>Username:</label>
  <input id="username" type="text" name="username">

  
  <label>Password:</label>
  <input id="password" type="password" name="password">
  
  <input type="submit" value="Log in!">
  
</form>